<configuration>
  <system.webServer>


    <handlers>
      <add name="iisnode" path="app.js" verb="*" modules="iisnode" />
    </handlers>


    <rewrite>
      <rules>
        <rule name="mongo">
          <match url="mongo/*" />
          <action type="Rewrite" url="app.js" />
        </rule>
      </rules>
    </rewrite>
        <directoryBrowse enabled="true" />
    
  </system.webServer>
</configuration>
